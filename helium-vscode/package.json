{
  "name": "helium-language",
  "publisher": "JoaoSantos",
  "displayName": "helium-language",
  "description": "Add vscode support for the helium language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CyberPhoenix90/helium"
  },
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "helium",
        "aliases": [
          "helium"
        ],
        "extensions": [
          ".helium",
          ".he"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "helium",
        "scopeName": "source.helium",
        "path": "./syntaxes/helium.tmLanguage.json"
      }
    ]
  }
}
